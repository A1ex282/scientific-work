procedure TForm2.ComboBox2Change(Sender: TObject);
var
  i, j : integer;
begin
If ComboBox2.ItemIndex = 0 then {100 μμ}

  z:=32;
  for i:=0 to 1 do
  for j:=0 to z do
  If ComboBox1.ItemIndex = 0 then
  u[i,j]:= s11[i,j];

    z:=26;
    for i:=0 to 1 do
    for j:=0 to z do
    If ComboBox1.ItemIndex = 2 then  u[i,j]:= s21[i,j];

If ComboBox2.ItemIndex = 1 then {150 μμ}

   z:=32;
   for i:=0 to 1 do
   for j:=0 to z do
   If ComboBox1.ItemIndex = 1 then  u[i,j]:= s15[i,j];

end;

