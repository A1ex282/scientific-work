{$R *.dfm}

uses Unit1;


procedure TForm2.ComboBox2Change(Sender: TObject);
var
  i, j : integer;
begin
If ComboBox2.ItemIndex = 0 then {100 μμ}

  z:=32;
  for i:=0 to 1 do
  for j:=0 to z do
  If ComboBox1.ItemIndex = 0 then
  u[i,j]:= s11[i,j];

    z:=26;
    for i:=0 to 1 do
    for j:=0 to z do
    If ComboBox1.ItemIndex = 1 then  u[i,j]:= s21[i,j];

If ComboBox2.ItemIndex = 1 then {150 μμ}

   z:=32;
   for i:=0 to 1 do
   for j:=0 to z do
   If ComboBox1.ItemIndex = 0 then  u[i,j]:= s12[i,j];

end;

procedure TForm2.Button1Click(Sender: TObject);
Var
j : integer;
begin
Series1.Clear;
  begin
    for j:=0 to z do
    Series1.Addxy(u[0,j],u[1,j]);
  end;
end;

end.


